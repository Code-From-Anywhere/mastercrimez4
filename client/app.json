{
  "expo": {
    "name": "MasterCrimeZ",
    "slug": "mastercrimez",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "4.14",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.wwoessi.mastercrimez",
      "buildNumber": "14",
      "associatedDomains": ["applinks:mastercrimez.com"]
    },
    "android": {
      "package": "com.wwoessi.mastercrimez",
      "useNextNotificationsApi": true,
      "versionCode": 14,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.mastercrimez.com",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    }
  }
}
